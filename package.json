{
  "name": "claude-relais-skill",
  "version": "1.0.10",
  "description": "Legacy internal engine for claude-relais skill workflows",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "claude-relais": "dist/index.js"
  },
  "files": [
    "README.md",
    "SKILL.md",
    "LICENSE",
    "agents/**",
    "references/**",
    "scripts/install.sh",
    "scripts/preflight.sh",
    "scripts/smoke.sh",
    "scripts/uninstall.sh"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "pack:check": "node scripts/pack-check.mjs",
    "prepack": "npm run -s build && node scripts/pack-check.mjs",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "pnpm -s build && vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "claude",
    "orchestration",
    "ai",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "ajv": "^8.18.0",
    "commander": "^14.0.2",
    "micromatch": "^4.0.8"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.9",
    "@types/node": "^25.0.10",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
